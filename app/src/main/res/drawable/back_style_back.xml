<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/_2sdp"
                android:color="#A6A5A5"/>
            <solid android:color="#058074"/>
            <corners android:radius="@dimen/_8sdp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/_2sdp"
                android:color="#063A41"/>
            <solid android:color="@android:drawable/screen_background_light_transparent"/>
            <corners android:radius="@dimen/_5sdp"/>

        </shape>
    </item>
</selector>